<%- include("partials/header") -%>


<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label>Title</label>
        <input name="postTitle" type="text" class="form-control" id="exampleFormControlInput1" autocomplete="off">
    </div>

    <div class="form-group">
        <label>Post</label>
        <textarea name="postBody" class="form-control" id="exampleFormControlTextarea1" rows="6"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="submit">Publish</button>
</form>

<%- include("partials/footer") -%>